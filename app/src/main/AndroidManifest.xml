<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sag.tessera.tessera">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".startup_splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main_menu" />
        <activity android:name=".settings_page" />
        <activity android:name=".profile_page" />
        <activity android:name=".level1_splash" />
        <activity android:name=".level1" />
        <activity android:name=".result" />
        <activity android:name=".wonResult_page" />
        <activity android:name=".level2_splash" />
        <activity android:name=".level2" />
        <activity android:name=".wonResult_page2" />
        <activity android:name=".level3_splash" />
        <activity android:name=".level3" />
        <activity android:name=".wonResult_page3" />
        <activity android:name=".level4" />
        <activity android:name=".wonResult_page4" />
        <activity android:name=".end_splash" />
        <activity android:name=".level4_splash" />
        <activity android:name=".result2" />
        <activity android:name=".result3" />
        <activity android:name=".result4" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".register_login" />
        <activity android:name=".register" />
        <activity android:name=".registration_tier2" />
        <activity android:name=".registration_tier3" />
        <activity android:name=".login" />
        <activity android:name=".objective_page" />
        <activity android:name=".user_profile" />
        <activity android:name=".view_scores"></activity>
    </application>

</manifest>